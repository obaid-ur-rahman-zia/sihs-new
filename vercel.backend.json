{
  "version": 2,
  "builds": [
    {
      "src": "src/server/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/src/server/index.js"
    },
    {
      "src": "/uploads/(.*)",
      "dest": "/src/server/index.js"
    }
  ],
  "env": {
    "NODE_ENV": "production",
    "USE_CLOUDINARY": "true",
    "VERCEL_ENV": "1"
  }
}